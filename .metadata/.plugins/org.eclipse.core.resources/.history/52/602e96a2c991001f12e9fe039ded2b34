package vo;

public class MyProfile {
	private String name;
	private int age;
	private boolean method;
	public MyProfile(String name, int age, String phone, boolean method) {
		super();
		this.name = name;
		this.age = age;
		this.method = method;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}

	public boolean isMethod() {
		return method;
	}
	public void setMethod(boolean method) {
		this.method = method;
	}
	
	

}
